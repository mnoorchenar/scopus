@ARTICLE{Douville202552,
	author = {Douville, Nicholas J. and Bastarache, Lisa and He, Jing and Wu, Kuan-Han H. and Vanderwerff, Brett and Bertucci-Richter, Emily and Hornsby, Whitney E. and Lewis, Adam and Jewell, Elizabeth S. and Kheterpal, Sachin and Shah, Nirav and Mathis, Michael and Engoren, Milo C. and Douville, Christopher B. and Surakka, Ida and Willer, Cristen and Kertai, Miklos D.},
