	year = {2006},
	journal = {Journal of the American Academy of Nurse Practitioners},
	volume = {18},
	number = {8},
	pages = {386 – 392},
	doi = {10.1111/j.1745-7599.2006.00152.x},
	abstract = {To examine the relationship between demographic and clinical characteristics of cardiac surgery patients with postoperative length of stay (PLOS) greater than 7 days and determine the demographic, social, and clinical predictors of the need for transitional cardiac rehabilitation (TCR) after cardiac surgery. A retrospective review of characteristics, clinical indices, caregiver availability, and patient status (whether living alone) was completed for 304 patients undergoing cardiac surgery over 24 consecutive months. Univariate analyses and multivariable logistic regression models were used to evaluate risk factor characteristics for PLOS greater than 7 days and to predict discharge disposition to TCR or home. Older patients, those with preoperative comorbidities, and those without a caregiver at home experience delays in functional recovery and discharge and are more likely to need TCR services. Our findings support the addition of functional recovery and social support risk items to the preoperative cardiac surgery risk assessment. © 2006 American Academy of Nurse Practitioners.},
	type = {Article},
	publication_stage = {Final},
	note = {Cited by: 17}
}
@ARTICLE{Aspi2020490,
