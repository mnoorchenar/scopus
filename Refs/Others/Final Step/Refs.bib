@ARTICLE{Fang2023,
	author = {Fang, Fuquan and Liu, Tiantian and Li, Jun and Yang, Yanchang and Hang, Wenxin and Yan, Dandan and Ye, Sujuan and Wu, Pin and Hu, Yuhan and Hu, Zhiyong},
	title = {A novel nomogram for predicting the prolonged length of stay in post-anesthesia care unit after elective operation},
	year = {2023},
	journal = {BMC Anesthesiology},
